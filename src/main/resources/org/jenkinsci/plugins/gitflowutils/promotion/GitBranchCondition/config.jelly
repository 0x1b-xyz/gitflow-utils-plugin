<!-- nothing to configure -->
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    <f:entry title="Branch Patterns">
        <f:textbox name="patterns" value="${instance.patterns}" />
    </f:entry>
    <f:entry>
        <f:checkbox name="evenIfUnstable" checked="${instance.evenIfUnstable}"/>
        <label class="attach-previous">${%Trigger even if the build is unstable}</label>
    </f:entry>
</j:jelly>